func raiseError() {
    return 1 / 0;
}

func map(fn, data) {
    if (!(fn is Function)) {
        System.print(fn);
        raiseError();
    }
    System.print('goos');

    var results = [];
    var idx = 0;

    for (var item in data) {
        results[idx] = fn(item, idx);
        idx += 1;
    }

    return results;
}
